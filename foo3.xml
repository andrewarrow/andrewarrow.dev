<?xml version="1.0" encoding="utf-8"?>
<stagenow>
  <profile>
    <title>RFID Access (Imported)</title>
    <description>AccessMgr + (optional) AppMgr/IntentMgr</description>
    <clientversion>5.0.0.0</clientversion>
    <mxversion>11.5</mxversion>
    <devicetype>Android</devicetype>
    <settings>{"profile":{"name":"RFID Access (Imported)","mx":"11.5","csp":["AccessMgr","AppMgr","IntentMgr"]}}</settings>

<wap-provisioningdoc>
  <characteristic type="AppMgr" version="11.5">
    <parm name="Action" value="Whitelist"/>
    <parm name="Package" value="io.trypura.purascan.debug"/>
  </characteristic>
  <characteristic type="AccessMgr">
    <parm name="ServiceAction" value="AllowCaller"/>
    <parm name="ServiceIdentifier" value="vendor.zebra.hardware.rfidhost-service"/>
    <parm name="CallerPackageName" value="io.trypura.purascan.debug"/>
    <parm name="CallerSignature" value="-----BEGIN CERTIFICATE----------END CERTIFICATE-----"/>
  </characteristic>
  <characteristic type="AccessMgr">
    <parm name="ServiceAction" value="AllowCaller"/>
    <parm name="ServiceIdentifier" value="vendor.zebra.hardware.rfidserial-service"/>
    <parm name="CallerPackageName" value="io.trypura.purascan.debug"/>
    <parm name="CallerSignature" value="-----BEGIN CERTIFICATE----------END CERTIFICATE-----"/>
  </characteristic>
  <characteristic type="AppMgr" version="11.5">
    <parm name="Action" value="GrantRuntimePermission"/>
    <parm name="Package" value="io.trypura.purascan.debug"/>
    <parm name="AndroidPermission" value="android.permission.BLUETOOTH_CONNECT"/>
  </characteristic>
  <characteristic type="AppMgr" version="11.5">
    <parm name="Action" value="GrantRuntimePermission"/>
    <parm name="Package" value="io.trypura.purascan.debug"/>
    <parm name="AndroidPermission" value="android.permission.BLUETOOTH_SCAN"/>
  </characteristic>
  <characteristic type="AppMgr" version="11.5">
    <parm name="Action" value="GrantRuntimePermission"/>
    <parm name="Package" value="io.trypura.purascan.debug"/>
    <parm name="AndroidPermission" value="android.permission.ACCESS_FINE_LOCATION"/>
  </characteristic>
  <characteristic type="IntentMgr">
    <parm name="Action" value="AllowIntentAction"/>
    <parm name="IntentAction" value="com.symbol.datawedge.api.ACTION"/>
    <parm name="CallerPackageName" value="io.trypura.purascan.debug"/>
  </characteristic>
    </wap-provisioningdoc>
  </profile>
</stagenow>
